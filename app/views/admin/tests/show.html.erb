<h1><%= t('.header', title: @test.title) %></h1>

<p>
  <strong><%= t('.title') %>:</strong>
  <%= @test.title %>
</p>

<p>
  <strong><%= t('.level') %>:</strong>
  <%= @test.level %>
</p>

<p><%= link_to t('.new_question'), new_test_question_path(@test) %></p>

<table>
  <tr>
    <th><%= t('.questions') %>:</th>
    <th colspan="3"><%= t('.actions') %></th>
    <%= render @test.questions %>
  </tr>
</table>

<p><%= link_to t('Edit'), edit_admin_test_path %></p>
<p><%= link_to t('Back'), admin_tests_path %></p>
